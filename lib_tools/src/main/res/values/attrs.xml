<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CardLayout">
        <attr name="widthRatio" format="float" />
        <attr name="heightRatio" format="float" />
        <attr name="cornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="GridPlacePicker">
        <attr name="column" format="integer" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="android:gravity" />
        <attr name="fixFlowLines" format="integer" />
    </declare-styleable>

    <declare-styleable name="FlowLayout_Layout">
        <attr name="android:layout_gravity" />
    </declare-styleable>

</resources>
